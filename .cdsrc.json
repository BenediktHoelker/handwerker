{
    "requires": {
        "auth": {
            "[development]": {
                "kind": "dummy"
            },
            "[test, production]": {
                "impl": "srv/auth.js"
            }
        },
        "db": {
            "kind": "postgres",
            "credentials": {
                "[test, production]": {
                    "ssl": {
                        "rejectUnauthorized": false
                    }
                }
            }
        },
        "postgres": {
            "dialect": "plain",
            "impl": "cds-pg",
            "model": [
                "srv"
            ]
        }
    },
    "folders": {
        "[development]": {
            "app": "app"
        },
        "[test, production]": {
            "folders": {
                "app": "dist"
            }
        }
    }
}
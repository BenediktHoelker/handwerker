{
    "requires": {
        "auth": {
            "[development]": {
                "kind": "dummy"
            },
            "[basic]": {
                "kind": "basic",
                "users": {
                    "alice": {}
                }
            },
            "[test]": {
                "impl": "srv/auth.js"
            },
            "[production]": {
                "impl": "srv/auth.js"
            }
        },
        "db": {
            "kind": "postgres",
            "credentials": {
                "[test]": {
                    "ssl": {
                        "rejectUnauthorized": false
                    }
                },
                "[production]": {
                    "ssl": {
                        "rejectUnauthorized": false
                    }
                }
            }
        },
        "postgres": {
            "dialect": "plain",
            "impl": "cds-pg",
            "model": [
                "srv"
            ]
        }
    },
    "folders": {
        "[development]": {
            "app": "app"
        },
        "[test]": {
            "app": "dist"
        },
        "[production]": {
            "app": "dist"
        }
    }
}
<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:upload="sap.m.upload"
	xmlns:mvc="sap.ui.core.mvc">
	<Dialog id="fileUploadDialog" showHeader="false" contentHeight="560px" contentWidth="480px">
		<upload:UploadSet id="uploadSet" instantUpload="false" uploadEnabled="true" afterItemAdded="onAfterItemAdded" uploadCompleted="onUploadCompleted" />
		<endButton>
			<Button type="Emphasized" text="{i18n>ok}"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>

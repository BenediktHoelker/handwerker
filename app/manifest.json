{
	"_version": "1.12.0",
	"sap.app": {
		"id": "handwerker",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/v2/handwerker/",
				"type": "OData"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "handwerker.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.108.11",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "handwerker.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "handwerker.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"name": "editOrderItems",
					"pattern": "editOrderItems",
					"target": {
						"name": "editOrderItems",
						"prefix": "eoi"
					}
				},
				{
					"pattern": "",
					"name": "editOrders",
					"target": {
						"name": "editOrders",
						"prefix": "eo"
					}
				},
				{
					"pattern": "manageBusinessPartners",
					"name": "manageBusinessPartners",
					"target": {
						"name": "manageBusinessPartners",
						"prefix": "mngBP"
					}
				}
			],
			"targets": {
				"editOrderItems": {
					"type": "Component",
					"usage": "editOrderItemsComponent",
					"title": "Edit Order Items",
					"id": "editOrderItems"
				},
				"editOrders": {
					"type": "Component",
					"usage": "editOrdersComponent",
					"title": "Edit Orders",
					"id": "editOrders"
				},
				"manageBusinessPartners": {
					"type": "Component",
					"usage": "manageBusinessPartnersComponent",
					"title": "Manage Business Partners",
					"id": "manageBusinessPartners"
				}
			}
		},
		"componentUsages": {
			"editOrderItemsComponent": {
				"name": "handwerker.components.orderitems",
				"settings": {},
				"componentData": {},
				"lazy": true
			},
			"editOrdersComponent": {
				"name": "handwerker.components.orders",
				"settings": {},
				"componentData": {},
				"lazy": true
			},
			"manageBusinessPartnersComponent": {
				"name": "handwerker.components.businesspartners",
				"settings": {},
				"componentData": {},
				"lazy": true
			}
		}
	}
}